<VSTemplate Version="3.0.0" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005" Type="Project">
  <TemplateData>
    <Name>UrhoSharp.Droid</Name>
    <Description>&lt;No description available&gt;</Description>
    <ProjectType>CSharp</ProjectType>
    <ProjectSubType>
    </ProjectSubType>
    <SortOrder>1002</SortOrder>
    <CreateNewFolder>true</CreateNewFolder>
    <DefaultName>UrhoSharp.Droid</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <LocationField>Enabled</LocationField>
    <EnableLocationBrowseButton>true</EnableLocationBrowseButton>
    <Icon>__TemplateIcon.ico</Icon>
  </TemplateData>
  <TemplateContent>
    <Project TargetFileName="UrhoSharp.Droid.csproj" File="UrhoSharp.Droid.csproj" ReplaceParameters="true">
      <Folder Name="Assets" TargetFolderName="Assets">
      <Folder Name="MyData" TargetFolderName="MyData">
        <Folder Name="Materials" TargetFolderName="Materials">
          <ProjectItem ReplaceParameters="false" TargetFileName="Earth.xml">Earth.xml</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="Moon.xml">Moon.xml</ProjectItem>
        </Folder>
        <Folder Name="Textures" TargetFolderName="Textures">
          <ProjectItem ReplaceParameters="false" TargetFileName="Earth.jpg">Earth.jpg</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="Earth_Clouds.jpg">Earth_Clouds.jpg</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="Earth_Night.jpg">Earth_Night.jpg</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="Earth_NormalsMap.png">Earth_NormalsMap.png</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="Earth_SpecularMap.png">Earth_SpecularMap.png</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="Moon.jpg">Moon.jpg</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="Moon_NormalsMap.png">Moon_NormalsMap.png</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="Space.png">Space.png</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="xamarin.png">xamarin.png</ProjectItem>
          <ProjectItem ReplaceParameters="false" TargetFileName="License.txt">License.txt</ProjectItem>
        </Folder>
      </Folder>
      </Folder>
      <ProjectItem ReplaceParameters="true" TargetFileName="MainActivity.cs">MainActivity.cs</ProjectItem>
      <ProjectItem ReplaceParameters="true" TargetFileName="packages.config">packages.config</ProjectItem>
      <Folder Name="Properties" TargetFolderName="Properties">
        <ProjectItem ReplaceParameters="true" TargetFileName="AndroidManifest.xml">AndroidManifest.xml</ProjectItem>
        <ProjectItem ReplaceParameters="true" TargetFileName="AssemblyInfo.cs">AssemblyInfo.cs</ProjectItem>
      </Folder>
      <Folder Name="Resources" TargetFolderName="Resources">
        <Folder Name="drawable" TargetFolderName="drawable">
          <ProjectItem ReplaceParameters="false" TargetFileName="Icon.png">Icon.png</ProjectItem>
        </Folder>
        <ProjectItem ReplaceParameters="true" TargetFileName="Resource.Designer.cs">Resource.Designer.cs</ProjectItem>
      </Folder>
    </Project>
  </TemplateContent>
    <WizardExtension>
    <Assembly>UrhoSharpTemplateWizard, Version=1.0.0.0, Culture=neutral, PublicKeyToken=f37fbcd47a310c3f</Assembly>
    <FullClassName>UrhoSharpTemplateWizard.ChildWizard</FullClassName>
  </WizardExtension>
</VSTemplate>